{
    "name": "mythicalsystems/pterodactyl-php-sdk",
    "description": "Modern, comprehensive PHP SDK for Pterodactyl Panel API - Complete replacement for abandoned/outdated Pterodactyl SDKs with full Admin, Client & Wings API support",
    "type": "library",
    "keywords": ["pterodactyl", "api", "sdk", "php", "panel", "game-server", "replacement", "modern", "comprehensive", "admin", "client", "wings"],
    "homepage": "https://github.com/MythicalLTD/Pterodactyl-PHP-SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Cassian Gherman",
            "email": "cassian@mythical.systems",
            "role": "Developer"
        },
        {
            "name": "MythicalSystems",
            "email": "contact@mythical.systems",
            "role": "Organization"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MythicalSystems\\SDK\\Pterodactyl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MythicalSystems\\SDK\\Pterodactyl\\Tests\\": "tests/"
        }
    },
    "replace": {
        "hcgcloud/pterodactyl-sdk": "*",
        "vexura/pterodactyl-sdk": "*",
        "prepaid-host/pterodactyl-sdk": "*",
        "fruitbytes/pterodactyl-sdk": "*",
        "emeraldhost/pterodactyl-sdk": "*",
        "sva-su/pterodactyl-php-sdk": "*",
        "pjz9n/pterodactyl-php-sdk": "*",
        "patryqhyper/pterodactyl-php-sdk": "*",
        "the-systems/pterodactyl-sdk": "*",
        "jeffwatsongh/pterodactyl-sdk": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "analyse": "phpstan analyse",
        "cs:check": "phpcs",
        "cs:fix": "phpcbf",
        "quality": [
            "@cs:check",
            "@analyse",
            "@test"
        ]
    }
}
